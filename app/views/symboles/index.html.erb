<h1>Listing Symboles</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @symboles.each do |symbole| %>
      <tr>
        <td><%= link_to 'Show', symbole %></td>
        <td><%= link_to 'Edit', edit_symbole_path(symbole) %></td>
        <td><%= link_to 'Destroy', symbole, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Symbole', new_symbole_path %>
